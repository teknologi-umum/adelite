{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["@teknologi-umum/eslint-config-typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": ["builtin", ["external", "internal"], "parent", ["sibling", "index"]]
      }
    ]
  }
}